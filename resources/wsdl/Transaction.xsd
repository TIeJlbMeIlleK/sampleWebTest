               <xsd:documentation>Сумма операции</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:choice>
            <xsd:element name="PayeePhone" type="xsd:string" minOccurs="0">
               <xsd:annotation>
                  <xsd:documentation>Номер телефона получателя</xsd:documentation>
               </xsd:annotation>
            </xsd:element>
            <xsd:sequence>
               <xsd:element name="BIK" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>БИК банка получателя</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="PayeeAccount" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Номер счета списания</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
            <xsd:element name="DestinationCardNumber" type="xsd:string" minOccurs="0">
               <xsd:annotation>
                  <xsd:documentation>Номер карты получателя</xsd:documentation>
               </xsd:annotation>
            </xsd:element>
         </xsd:choice>
         <xsd:element name="BankName" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Банк получателя платежа</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PayeeName" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Получатель платежа</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CommissionAmount" type="xsd:decimal" nillable="true" default="-1">
            <xsd:annotation>
               <xsd:documentation>Комиссия</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SourceProduct" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Номер счета списания</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SourceCardNumber" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Номер карты списания</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SBP_id" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Идентификатор операции СБП</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OperationDescription" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Сообщение получателю</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="InsuredPersonPropsType">
      <xsd:sequence>
         <xsd:element name="FullName" type="xsd:string" nillable="false">
            <xsd:annotation>
               <xsd:documentation>ФИО</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DateOfBirth" type="xsd:date" nillable="false">
            <xsd:annotation>
               <xsd:documentation>Дата рождения</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RegAddress" type="xsd:string" nillable="false">
