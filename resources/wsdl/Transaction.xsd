               <xsd:documentation>Тип запроса</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="GettingCreditType">
      <xsd:annotation>
         <xsd:documentation>Запрос на выдачу кредита</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Filial" type="xsd:string" nillable="false">
            <xsd:annotation>
               <xsd:documentation>Счет зачисления средств</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OrderType" nillable="false">
            <xsd:annotation>
               <xsd:documentation>Тип распоряжения 4 – закрытие вклада 5 – закрытие текущего счета
                  6 – закрытие карточного счета 7 - закрытие металлического счета 8 – выдача кредита
               </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="xsd:int">
                  <xsd:maxInclusive value="8"/>
                  <xsd:minInclusive value="4"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="SourceProduct" type="xsd:string" nillable="true">
            <xsd:annotation>
               <xsd:documentation>Номер счета/вклада списания</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DestinationProduct" type="xsd:string" nillable="false">
            <xsd:annotation>
               <xsd:documentation>Счет зачисления средств</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DestinationCurrencyIsoCode" type="saft:CurrencyIsoCode">
            <xsd:annotation>
               <xsd:documentation>Валюта счета зачисления</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AmountInSourceCurrency" type="xsd:decimal"  nillable="false">
            <xsd:annotation>
               <xsd:documentation>Сумма кредита/кредитного лимита, которую выбрал клиент</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SourceCurrencyIsoCode" type="saft:CurrencyIsoCode" nillable="false">
            <xsd:annotation>
               <xsd:documentation>Валюта счета списания. Только для кредитов</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ApprovedLoanSum" type="xsd:long" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Максимальная одобренная сумма кредита (кредит выдается без копеек)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="InsuranceFee" type="xsd:decimal" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Сумма страховки</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="LoanRate" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Процентная ставка по кредиту</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="LoanType" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Тип кредита (кредит/кредитная карта)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ProductName" type="xsd:string" nillable="false">
            <xsd:annotation>
               <xsd:documentation>Продукт</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Period" type="xsd:int">
            <xsd:annotation>
               <xsd:documentation>Срок кредита в месяцах</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="RequestCardReqType" abstract="true">
      <xsd:sequence>
         <xsd:element name="SourceCardNumber" type="xsd:string" nillable="false" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Полный номер карты клиента, запросившего реквизиты</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="RequestPANType">
      <xsd:complexContent>
         <xsd:extension base="saft:RequestCardReqType"/>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="RequestCCVType">
      <xsd:complexContent>
         <xsd:extension base="saft:RequestCardReqType"/>
      </xsd:complexContent>
