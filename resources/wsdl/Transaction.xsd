   <xsd:complexType name="TransactionCancellationType">
      <xsd:annotation>
         <xsd:documentation>Отмена операции</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="transactionIdToCancel" type="xsd:long"/>
