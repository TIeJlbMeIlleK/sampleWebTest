   <xsd:complexType name="TradingFloorReplenishmentType">
      <xsd:annotation>
         <xsd:documentation>Пополнение торговой площадки</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="SourceProduct" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Источник списания</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DestinationProduct" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Брокерский счет</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TradingFloorName" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Наименование торговой площадки</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AmountInSourceCurrency" type="xsd:decimal">
            <xsd:annotation>
               <xsd:documentation>Сумма в валюте списания</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CommissionAmount" type="xsd:decimal" nillable="true" default="-1">
            <xsd:annotation>
               <xsd:documentation>Комиссия в валюте списания</xsd:documentation>
