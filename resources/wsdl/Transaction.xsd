   <xsd:complexType name="PaymentC2B">
      <xsd:sequence>
         <xsd:element name="qrCodeID" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Идентификатор операции в СБП</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SourceProduct" type="xsd:string" nillable="false">
            <xsd:annotation>
               <xsd:documentation>Номер карточного счета списания</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AmountInSourceCurrency" type="xsd:decimal" nillable="false">
            <xsd:annotation>
               <xsd:documentation>Сумма операции в валюте счёта списания без учёта комиссии за её совершение.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SourceCurrencyIsoCode" type="saft:CurrencyIsoCode" nillable="false">
            <xsd:annotation>
               <xsd:documentation>Валюта, всегда российский рубль</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CommissionAmount" type="xsd:decimal" nillable="true">
            <xsd:annotation>
               <xsd:documentation>Комиссия за перевод
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PayerINN" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>ИНН плательщика</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PayeeName" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Наименование юридического лица получателя</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PayeeINN" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>ИНН Получателя перевода</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TSPName" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Название ТСП</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TSPType" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Категория ТСП</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PayeeAddress" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Адрес получателя</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="BIK" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Идентификатор банка получателя</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OperationDescription" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Назначение платежа</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="qrCodeType" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Тип QR-кода</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
</xsd:schema>
