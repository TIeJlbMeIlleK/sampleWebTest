   <xsd:complexType name="RequestForGosuslugiType">
      <xsd:annotation>
         <xsd:documentation>Запрос в госуслуги</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="AuthenticationType" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Способ доставки пароля для первого входа в систему</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="GosuslugiRequestType" type="xsd:int">
